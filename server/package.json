{
    "name": "movie-book",
    "version": "1.0.0",
    "description": "Backend Of the MovieBook App",
    "main": "src/server.ts",
    "type": "module",
    "scripts": {
        "build": "rm -rf dist/* && npx tsc",
        "prestart": "npm run build",
        "start": "node dist/server.js",
        "devTS": "NODE_ENV=development NODE_OPTIONS='--loader ts-node/esm' ts-node ./src/server.ts",
        "preprod": "npm run build",
        "prod": "NODE_ENV=production node dist/server.js",
        "nodemon": "nodemon",
        "local": "tsc-watch --onSuccess='nodemon dist/server.js' --watch src,server.ts"
    },
    "keywords": [],
    "author": "Utsav Kachchhi, Harshvardhan Makwana, Dodi Tank",
    "license": "ISC",
    "dependencies": {
        "@prisma/client": "^5.15.1",
        "cors": "^2.8.5",
        "dotenv": "^16.4.5",
        "express": "^4.19.2",
        "prisma": "^5.15.1"
    },
    "devDependencies": {
        "@types/cors": "^2.8.17",
        "@types/express": "^4.17.21",
        "@types/node": "^20.14.2",
        "nodemon": "^3.1.4",
        "ts-node": "^10.9.2",
        "typescript": "^5.4.5"
    }
}
