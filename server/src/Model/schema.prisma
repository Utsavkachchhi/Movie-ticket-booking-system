
datasource db {
  provider = "mongodb"
  url      = env("DATABASE_URL")
}

generator client {
  provider = "prisma-client-js"
}
model User {
    id String @id @default(auto()) @map("_id") @db.ObjectId
    name String
    email String
    mobile Int?
    gender String?
    password String

    @@map("users")
}

model City{
id	String @id @default(auto()) @map("_id") @db.ObjectId
name	String
state	String
country	String
created_at DateTime?

@@map("city")
}